h2>Descripcion</h2>\n                <h2>Precio Unitario</h2>\n                <h2>Cantidad</h2>\n                <h2>Quitar</h2>\n            </div>\n            {cartListItems.map( (item) => {\n                const {id, title, image, price, cantidad} = item\n                return(\n                    <div className='cart-table__content' key={id}>\n                        <div>\n                            <img src={`./images/${image}`} />\n                        </div>\n                        <div className='cart-table__content-title'>\n                            <p>{title}</p>\n                        </div>\n                        <div className='cart-table__content-price'>\n                            <p>$ {price}</p>\n                        </div>\n                        <div className='cart-table__content-quantity'>\n                            <p>{cantidad}</p>\n                        </div>\n                        <div className='cart-table__content-price'>\n                            <button className='btn-delete' onClick={()=>deleteProduct(item)}>\n                                <Delete />\n                            </button>\n                        </div>\n                    </div>\n                )\n            })}\n            <div className='cart-footer'>\n                <Button><Link to={'/shop'} className='btn-custom' style={{textDecoration: 'none'}}>Continuar comprando</Link></Button>\n                <div className='cart-checkout-details'>\n                    <div className='cart-checkout__subtotal'>\n                        <p>Subtotal</p>\n                        <span>$ {totalPrice}</span>\n                    </div>\n                    <div className='cart-checkout__total'>\n                        <p>Total</p>\n                        <span>$ {totalPrice}</span>\n                    </div>\n                    <Button className='btn-custom' onClick={() => {confirmOrder()}}>Finalizar Compra</Button>\n                </div>